pipeline {
    agent any
    tools {
        maven 'maven'
    }
    // tools {
    //     nodejs 'nodejs'
    // }
    stages {
        stage('Build') {
            steps {
                bat 'mvn -B -DskipTests clean package'
            }
        }
        stage('Test') {
            steps {
                bat 'mvn test'
            }
        }
        stage('Deliver'){
            steps{
               bat './scripts/deliver.sh'
            }
        }
    }
}