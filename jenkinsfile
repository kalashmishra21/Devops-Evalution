pipeline {
    agent any
    tools {
        nodejs 'nodejs'
    }
    tools {
        maven 'maven'
    }
    stages {
        stage('Build') {
            steps {
                bat 'mvn -B -DskipTests clean package'
            }
        }
        stage('Test') {
            steps {
                bat 'mvn test'
            }
        }
        stage('Deliver'){
            steps{
               echo 'Delivered'
            }
        }
    }
}